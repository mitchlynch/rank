{
  "name": "rank",
  "version": "0.0.1",
  "description": "rank application",
  "main": "rank.js",
  "scripts": {
    "prestart": "./node_modules/.bin/babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch devbuild",
    "open:src": "./node_modules/.bin/babel-node server/devServer.js",
    "lint": "./node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "./node_modules/.bin/mocha --reporter progress tools/testSetup.js \"src/**/__test__/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "./node_modules/.bin/rimraf ./dist",
    "devbuild": "npm-run-all clean-dist build:html",
    "build:html": "./node_modules/.bin/babel-node tools/buildHtml.js",
    "mocha": "MOCHA_FILE=./test-reports/test-results.xml node_modules/.bin/mocha --reporter mocha-junit-reporter --outputDir test-reports tools/testSetup.js \"src/**/__test__/*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com..."
  },
